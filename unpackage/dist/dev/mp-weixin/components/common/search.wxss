
.container {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
}
.bar {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 68rpx;
  font-size: 28rpx;
  border-radius: 10rpx;
  background-color: #f5f5f5;
  padding-left: 60rpx;
}
.find {
  position: absolute;
  top: 50%;
  -webkit-transform: translate(16rpx, -50%);
          transform: translate(16rpx, -50%);
  color: #9f9f9f;
  font-size: 48rpx;
}

